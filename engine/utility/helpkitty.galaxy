//Lets help kat
const int max_achievements = 3;
const int max_players = 16;
string[max_players][max_achievements] my_achievements;

void init_achievements_ref(int p) {

    my_achievements[p][0] = "Achievement1";
    my_achievements[p][1] = "Achievement2";
    my_achievements[p][2] = "Achievement3";
}

string reference_achievment(int p, int achievement) {
    return my_achievements[p][achievement];
}