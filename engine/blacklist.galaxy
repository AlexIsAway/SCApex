

const string gv_LibapxBankSectionBlacklist = "blacklist";

void LibapxBlacklistDeprecatedRemoveBank(int p) {
	if(BankSectionExists(gv_LibapxPlayer[p].bankStorage, gv_LibapxBankSectionBlacklist)) {
		LibapxLogInfo(p, "Removed Blacklist Bank Section! _blacklist was removed.");
		BankSectionRemove(gv_LibapxPlayer[p].bankStorage, gv_LibapxBankSectionBlacklist); 
	}
}
